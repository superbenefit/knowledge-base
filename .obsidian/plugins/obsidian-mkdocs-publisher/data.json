      "inline": false,
      "exclude": [],
      "fields": [
        "functions",
        "qualities"
      ]
    },
    "links": {
      "internal": true,
      "unshared": true,
      "wiki": true,
      "slugify": "disable",
      "unlink": true,
      "relativePath": false,
      "textPrefix": ""
    }
  },
  "embed": {
    "attachments": true,
    "overrideAttachments": [],
    "keySendFile": [],
    "notes": true,
    "folder": "attachments",
    "convertEmbedToLinks": "keep",
    "charConvert": "->",
