{
  "github": {
    "user": "superbenefit",
    "repo": "knowledge-garden",
    "branch": "rpp-update",
    "automaticallyMergePR": false,
    "dryRun": {
      "enable": false,
      "folderName": "enveloppe"
    },
    "tokenPath": "%configDir%/plugins/%pluginID%/env",
    "api": {
      "tiersForApi": "Github Free/Pro/Team (default)",
      "hostname": ""
    },
    "workflow": {
      "commitMessage": "[Enveloppe] Update content",
      "name": ""
    },
    "otherRepo": [],
    "verifiedRepo": true,
    "rateLimit": 0
