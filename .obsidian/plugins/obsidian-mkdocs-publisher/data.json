    "forcePush": true,
    "useObsidianFolder": true,
    "bake": {
      "textBefore": "",
      "textAfter": ""
    }
  },
  "plugin": {
    "shareKey": "publish",
    "fileMenu": true,
    "editorMenu": true,
    "excludedFolder": [
      "_drafts",
      "_tools"
    ],
    "copyLink": {
      "enable": false,
      "links": "",
      "removePart": [],
      "addCmd": false,
      "transform": {
